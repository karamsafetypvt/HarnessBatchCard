<script src="~/Template/bootstrapdemos.wrappixel.com/spike-tailwind/dist/assets/libs/iconify-icon/dist/iconify-icon.min.js"></script>
<script src="~/Template/bootstrapdemos.wrappixel.com/spike-tailwind/dist/assets/libs/preline/dist/preline.js"></script>
